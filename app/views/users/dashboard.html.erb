    .card {
        margin-top: 70px;
        display: inline-block;
        position: relative;
        width: 100%;
        height: 300px;
        opacity: 0.85;
    }
    .front,.front1,.back1,
    .back {
        width: 100%;
        height: 100%;
        position: absolute;
        -webkit-backface-visibility: hidden;
        backface-visibility: hidden;
        -webkit-transition: -webkit-transform 300ms;
        transition: -webkit-transform 300ms;
        transition: transform 300ms;
        transition: transform 300ms, -webkit-transform 300ms;
        -webkit-transition-timing-function: linear;
        transition-timing-function: linear;
        border-radius: 5px;
    }
    .front {
        transform: rotateY(0deg);
        background: orangered;
        text-align: center;
        padding: 50px;
    }
    .back {

        background: yellow;
        -webkit-transform: rotateY(180deg);
        transform: rotateY(180deg);
        padding: 10px 15px;
        color: #fff;
    }
    .front {
        transform: rotateY(0deg);
        background: orangered;
        text-align: center;
        padding: 30px;
    }
    .back {

        background: yellow;
        -webkit-transform: rotateY(180deg);
        transform: rotateY(180deg);
        padding: 40px 15px;
        color: #fff;
    }
    .card:hover .front {
        -webkit-transform: rotateY(-180deg);
        transform: rotateY(-180deg);
    }
    .card:hover .back {
        -webkit-transform: rotateY(0deg);
        transform: rotateY(0deg);
    }

      .front .contact {
          height: 120px;
          width: 120px;
          border: 5px solid #B2DBBF;
          border-radius: 50%;
          display: inline-block;
          margin-bottom: 10px;
      }
    .front .name {
        display: inline-block;
        font-size: 48px;
        color: #fff;
    }
    .front .job {
        color: #fff;
        font-size: 40px;
    }
    .front hr {
